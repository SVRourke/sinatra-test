<div class="col s4 m6 l4">
            <div class="card">
                <div class="card-image">
                    <img class="responsive-image" src="<%= product['image'] %>" alt="Product Image">
                </div>
                <span class=""><%= product['name'] %></span>

                <div class="card-content">
                    <span>SKU: <%= product['sku'] %></span>
                    <p>Avg Review: <%= product['customerReviewAverage'] %></p>
                    <p>Last Price Change: <%= product['priceUpdateDate'] %></p>
                    <p>Price: $<%= product['salePrice'] %></p>
                    <a class="btn halfway-fab red" href="<%= product['url'] %>">-$<%= product['dollarSavings'].to_i %></a>
                </div>
            </div>
        </div>